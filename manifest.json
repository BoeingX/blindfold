{
  "name": "Leetcode Blindfold",
  "description": "Reduce self imposed anxiety by hiding LeetCode's difficulty, acceptance, and question stats.",
  "version": "0.1",
  "background": {
    "scripts": ["main.js"]
  },
  "permissions": [
    "tabs", "http://leetcode.com/*", "https://leetcode.com/*"
  ],
  "browser_action": {
      "default_title": "Leetcode Blindfold"
  },
  "content_scripts": [
    {
      "matches": ["http://leetcode.com/*","https://leetcode.com/*"],
      "js": ["main.js"]
    }
  ],
  "manifest_version": 2
}
